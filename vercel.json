{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
 "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://api.dynamicdealerservices.com/api/$1"
    },
    {
      "src": "/(.*\\..*)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://api.dynamicdealerservices.com/api"
  },
  "build": {
    "env": {
      "VITE_API_BASE_URL": "https://api.dynamicdealerservices.com/api"
    }
  }
}

